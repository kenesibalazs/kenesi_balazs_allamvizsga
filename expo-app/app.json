{
  "expo": {
    "name": "expo-app",
    "slug": "expo-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./scr/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./scr/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.expo-app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./scr/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./scr/assets/favicon.png"
    }
  }
}
