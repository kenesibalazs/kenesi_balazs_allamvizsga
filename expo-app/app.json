{
  "expo": {
    "name": "Attendy",
    "slug": "expo-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./scr/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./scr/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kenesibalazs.expo-app",
      "infoPlist": {
        "CFBundleDisplayName": "Attendy"  
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./scr/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "READ_MEDIA_IMAGES",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.anonymous.expoapp"
    },
    "web": {
      "favicon": "./scr/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "549f2e95-0bb3-4ba6-a40c-a5063bc58bc2"
      }
    },
    "newArchEnabled": true

  }
}
